{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "JS",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8000/San_Pedro_DCTI_Bitacora/FormularioIngreso.php?ID=190",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",

            "sourceMapPathOverrides": {
            
                "webpack:///./*":"${workspaceRoot}/*" 
            }
        },       
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9000
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        }
    ]
}